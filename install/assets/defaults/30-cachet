#!/usr/bin/with-contenv bash

ADMIN_EMAIL=${ADMIN_EMAIL:-"admin@example.com"}
ADMIN_PASS=${ADMIN_PASS:-"cachet"}
ADMIN_USER=${ADMIN_USER:-"admin"}
APP_DEBUG=${APP_DEBUG:-"false"}
DB_PORT=${DB_PORT:-"3306"}
DB_TYPE=${DB_TYPE:-"mysql"}
DRIVER_CACHE=${DRIVER_CACHE:-"apc"}
DRIVER_QUEUE=${DRIVER_QUEUE:-"database"}
DRIVER_SESSION=${DRIVER_SESSION:-"apc"}
ENABLE_AUTO_UPDATE=${ENABLE_AUTO_UPDATE:-"TRUE"}
ENABLE_BEACON=${ENABLE_BEACON:-"FALSE"}
ENABLE_DEBUG_BAR=${ENABLE_DEBUG_BAR:-"FALSE"}
ENABLE_EMOJI=${ENABLE_EMOJI:-"FALSE"}
MAIL_FROM=${MAIL_FROM:-"noreply@example.com"}
MAIL_FROM_NAME=${MAIL_FROM_NAME:-"Cachet"}
REDIS_PORT=${REDIS_PORT:-"6379"}
SETUP_TYPE=${SETUP_TYPE:-"AUTO"}